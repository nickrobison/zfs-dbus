opam-version: "2.0"
name: "libzfs"
version: "~dev"
synopsis: "ZFS bindings"
maintainer: "Maintainer Name"
authors: "Author Name"
license: "LICENSE"
homepage: "https://github.com/username/reponame"
doc: "https://url/to/documentation"
bug-reports: "https://github.com/username/reponame/issues"
depends: [
  "alcotest" {= "1.7.0" & with-test}
  "alcotest-lwt" {= "1.7.0" & with-test}
  "astring" {= "0.8.5" & with-test}
  "base-bigarray" {= "base"}
  "base-bytes" {= "base" & with-test}
  "base-domains" {= "base"}
  "base-nnp" {= "base"}
  "base-threads" {= "base"}
  "base-unix" {= "base"}
  "bigarray-compat" {= "1.1.0"}
  "cmdliner" {= "1.3.0" & with-test}
  "conf-libffi" {= "2.0.0"}
  "conf-pkg-config" {= "3"}
  "cppo" {= "1.6.9"}
  "csexp" {= "1.5.2"}
  "ctypes" {= "0.22.0"}
  "ctypes-foreign" {= "0.22.0"}
  "dune" {= "3.16.0"}
  "dune-configurator" {= "3.16.0"}
  "fmt" {= "0.9.0" & with-test}
  "hmap" {= "0.8.1"}
  "host-arch-x86_64" {= "1"}
  "host-system-other" {= "1"}
  "integers" {= "0.7.0"}
  "logs" {= "0.7.0" & with-test}
  "lwt" {= "5.7.0" & with-test}
  "ocaml" {= "5.1.1"}
